<%- model_class = Demanda -%>
<div class="page-header">
  <h1><%=t '.title', :default => Demanda %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:descricao) %></dt>
      <dd><pre class="prettyprint"><%= @demanda.descricao %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:validade) %></dt>
      <dd><pre class="prettyprint"><%= @demanda.validade %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:cidade_origem) %></dt>
      <dd><pre class="prettyprint"><%= @demanda.cidade_origem %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:correspondente) %></dt>
      <dd><pre class="prettyprint"><%= @demanda.correspondente %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:status) %></dt>
      <dd><pre class="prettyprint"><%= @demanda.status %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              demandas_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_demanda_path(@demanda), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              demanda_path(@demanda),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

